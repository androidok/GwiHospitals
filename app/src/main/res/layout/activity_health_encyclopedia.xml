<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="false"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    tools:context="com.gwi.selfplatform.ui.activity.user.PersonalCenterV2Activity">

    <include layout="@layout/include_header" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TableLayout
                android:id="@+id/reg_select_v2_items_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TableRow
                    android:id="@+id/ly_disease_dictionary"
                    android:clickable="true">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/selector_image_inicator_background">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:padding="@dimen/android_padding_middle"
                            android:src="@drawable/selector_fill_peronal_detail" />
                    </FrameLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@drawable/selector_setting_item_grey_to_white"
                        android:drawableRight="@drawable/selector_arror_right"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/android_padding_normal"
                        android:paddingRight="@dimen/android_padding_middle"
                        android:text="@string/health_disease_dictionary"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@drawable/selector_item_text" />

                </TableRow>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey" />

                <TableRow
                    android:id="@+id/ly_drug_dictionary"
                    android:clickable="true">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/selector_image_inicator_background">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/android_padding_middle"
                            android:src="@drawable/selector_bookingform_1"
                            android:textColor="@drawable/selector_image_inicator_background" />
                    </FrameLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@drawable/selector_setting_item_grey_to_white"
                        android:drawableRight="@drawable/selector_arror_right"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/android_padding_normal"
                        android:paddingRight="@dimen/android_padding_middle"
                        android:text="@string/health_drug_dictionary"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@drawable/selector_item_text" />
                </TableRow>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey" />

                <TableRow
                    android:id="@+id/ly_first_aid"
                    android:clickable="true">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/selector_image_inicator_background">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/android_padding_middle"
                            android:src="@drawable/selector_bookingform_1" />
                    </FrameLayout>

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@drawable/selector_setting_item_grey_to_white"
                        android:drawableRight="@drawable/selector_arror_right"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/android_padding_normal"
                        android:paddingRight="@dimen/android_padding_middle"
                        android:text="@string/health_first_aid"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@drawable/selector_item_text" />
                </TableRow>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey" />

                <TableRow
                    android:id="@+id/ly_report_analysis"
                    android:clickable="true">

                    <FrameLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/selector_image_inicator_background">

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="@dimen/android_padding_middle"
                            android:src="@drawable/selector_bookingform_1" />
                    </FrameLayout>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="1"
                        android:background="@drawable/selector_setting_item_grey_to_white"
                        android:drawableRight="@drawable/selector_arror_right"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/android_padding_normal"
                        android:paddingRight="@dimen/android_padding_middle"
                        android:text="@string/health_report_analysis"
                        android:textAppearance="?android:textAppearanceMedium"
                        android:textColor="@drawable/selector_item_text" />
                </TableRow>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/grey" />
            </TableLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>
