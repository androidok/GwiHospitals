<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:clipToPadding="false"
    tools:context="com.gwi.selfplatform.ui.activity.registration.RegistCompleteActivity">

    <include layout="@layout/include_header" />

    <ViewStub
        android:id="@+id/regist_complete_error"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout="@layout/layout_regist_error"/>

    <include layout="@layout/include_loading_indicator" />
    <LinearLayout
        android:id="@+id/regist_complete_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/android_margin_middle"
        android:layout_marginLeft="@dimen/android_margin_normal"
        android:layout_marginRight="@dimen/android_margin_normal"
        android:paddingLeft="@dimen/android_padding_normal"
        android:paddingRight="@dimen/android_padding_normal"
        android:orientation="vertical">
        <TextView
            android:id="@+id/regist_complete_success_msg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/android_margin_normal"
            android:layout_marginRight="@dimen/android_margin_normal"
            android:layout_marginTop="@dimen/android_margin__big"
            android:text="@string/register_success_msg"/>

        <TableLayout
            android:id="@+id/regist_complete_info_layout"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/android_margin__big"
            android:layout_height="wrap_content">
            <TableRow>
                <TextView android:layout_width="0dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="@string/label_patient"/>
                <TextView
                    android:id="@+id/regist_complete_patient_name"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:gravity="left"
                    />
            </TableRow>
            <TableRow android:layout_marginTop="@dimen/android_margin_micro">
                <TextView android:layout_width="0dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="@string/label_card"/>
                <TextView android:layout_width="0dp"
                    android:id="@+id/regist_complete_card_no"
                    android:layout_weight="1"
                    android:gravity="left"
                    />
            </TableRow>
            <TableRow android:layout_marginTop="@dimen/android_margin_micro">
                <TextView android:layout_width="0dp"

                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="@string/label_balance"/>
                <TextView android:layout_width="0dp"
                    android:layout_weight="1"
                    android:id="@+id/regist_complete_card_balance"
                    android:gravity="left"
                    />
            </TableRow>
            <TableRow
                android:id="@+id/regist_complete_reg_password_layout"
                android:layout_marginTop="@dimen/android_margin_micro">
                <TextView android:layout_width="0dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="@string/label_regist_password"/>
                <TextView android:layout_width="0dp"
                    android:id="@+id/regist_complete_reg_password"
                    android:layout_weight="1"
                    android:gravity="left"
                    />
            </TableRow>

            <TableRow android:layout_marginTop="@dimen/android_margin_middle">
                <TextView android:layout_width="0dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="@string/label_depart"/>
                <TextView android:layout_width="0dp"
                    android:id="@+id/regist_complete_depart_name"
                    android:layout_weight="1"
                    android:gravity="left"
                    />
            </TableRow>
            <TableRow android:layout_marginTop="@dimen/android_margin_micro">
                <TextView android:layout_width="0dp"
                    android:layout_weight="1"
                    android:gravity="right"

                    android:text="@string/label_doctor"/>
                <TextView android:layout_width="0dp"
                    android:layout_weight="1"
                    android:id="@+id/regist_complete_dct_name"
                    android:gravity="left"
                    />
            </TableRow>
            <TableRow android:layout_marginTop="@dimen/android_margin_micro">
                <TextView android:layout_width="0dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="@string/label_look_position"/>
                <TextView android:layout_width="0dp"
                    android:id="@+id/regist_complete_depart_position"
                    android:layout_weight="1"
                    android:gravity="left"
                    />
            </TableRow>
            <TableRow android:layout_marginTop="@dimen/android_margin_micro">
                <TextView android:layout_width="0dp"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="@string/label_look_time"/>
                <TextView android:layout_width="0dp"
                    android:id="@+id/regist_complete_order_time"
                    android:layout_weight="1"
                    android:gravity="left"
                    />
            </TableRow>
        </TableLayout>
    </LinearLayout>



</LinearLayout>
