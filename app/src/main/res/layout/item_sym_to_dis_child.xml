<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <TextView android:id="@+id/sym_to_dis_child_memo"
        android:lineSpacingExtra="@dimen/android_padding_micro"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_sys_to_dis_child"
        android:textSize="@dimen/small_middle_text_size"
        android:textColor="@color/white"
        android:text="ffffffffffffffffffffffffff"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:background="#0096dc"
        android:paddingRight="@dimen/android_padding_normal"
        android:layout_height="wrap_content">
    <Button
        android:id="@+id/sym_to_dis_dept_name"
        android:layout_width="wrap_content"
        android:padding="@dimen/android_margin_micro"
        android:layout_margin="@dimen/android_margin_normal"
        android:background="@drawable/bg_corner_sym_to_dis"
        android:layout_gravity="right"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:text="心内科"/>
    </FrameLayout>
</LinearLayout>
